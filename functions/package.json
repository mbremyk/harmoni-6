{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "run-local": "nodemon index.js",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "engines": {
    "node": "8"
  },
  "jest": {
    "testRegex": "./tests/.+Test\\.js$",
    "testURL": "http://localhost"
  },
  "env": {
    "test": {
      "presets": "env"
    }
  },
  "dependencies": {
    "body-parser": "latest",
    "cors": "latest",
    "express": "latest",
    "express-fileupload": "^1.1.6",
    "express-multipart-file-parser": "^0.1.2",
    "file-reader": "^1.1.1",
    "firebase": "^7.6.2",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.3",
    "mysql": "^2.17.1",
    "bcryptjs": "^2.4.3",
    "is-ci": "latest",
    "jsonwebtoken": "latest",
    "moment": "latest",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "blob-util": "^2.0.2",
    "cross-blob": "^1.2.0",
    "express-busboy": "^7.0.1",
    "filereader": "^0.10.3",
    "firebase-functions-test": "^0.1.6",
    "formidable": "^1.2.1",
    "jest": "^24.9.0",
    "multer": "^1.4.2",
    "sequelize": "^5.21.3"
  },
  "private": true
}
